    <include
        android:id="@+id/include2"
        layout="@layout/app_bar_main" />

    <Button
        android:id="@+id/button"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:layout_marginTop="92dp"
        android:background="@drawable/ic_search_black_24dp" />

    <ScrollView
        android:layout_width="374dp"
        android:layout_height="516dp"
        android:layout_alignBottom="@+id/include2"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="0dp"
        android:layout_marginEnd="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="216dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#AB030303"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/vdo1"
                android:layout_width="match_parent"
                android:layout_height="166dp"
                android:background="#000000"
                android:baselineAlignBottom="false"
                android:padding="1dp"
                app:srcCompat="@mipmap/maxresdefault" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="254dp"
                    android:layout_height="73dp"
                    android:text="Post match conference  NZvsENG  | ICC Cricket WorldCup 2019"
                    android:textColor="#FFFFFF"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton6"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:background="#00FFFFFF"
                    app:srcCompat="@drawable/ic_thumbs_up_down_black_24dp"
                    tools:ignore="ContentDescription,VectorDrawableCompat" />

                <ImageButton
                    android:id="@+id/imageButton7"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    app:srcCompat="@android:drawable/btn_star_big_on" />

            </LinearLayout>

            <ImageView
                android:id="@+id/vdo2"
                android:layout_width="match_parent"
                android:layout_height="166dp"
                android:background="#FC1B1B1B"
                app:srcCompat="@mipmap/maxresdefault2"
                tools:ignore="ContentDescription" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="254dp"
                    android:layout_height="73dp"
                    android:text="Englan lifts theworldcup after a thriller | ICC Cricket Worldcup 2019"
                    android:textColor="#FAFFFFFF"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton9"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="#00FFFFFF"
                    app:srcCompat="@drawable/ic_thumbs_up_down_black_24dp"
                    tools:ignore="VectorDrawableCompat" />

                <ImageButton
                    android:id="@+id/imageButton8"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    app:srcCompat="@android:drawable/btn_star_big_on" />

            </LinearLayout>

            <ImageView
                android:id="@+id/vdo3"
                android:layout_width="match_parent"
                android:layout_height="166dp"
                android:background="#FD050505"
                app:srcCompat="@mipmap/sl" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="254dp"
                    android:layout_height="73dp"
                    android:text="Englan lifts theworldcup after a thriller | ICC Cricket Worldcup 2019"
                    android:textColor="#FAFFFFFF"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton10"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:background="#00FFFFFF"
                    app:srcCompat="@drawable/ic_thumbs_up_down_black_24dp"
                    tools:ignore="VectorDrawableCompat" />

                <ImageButton
                    android:id="@+id/imageButton11"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    app:srcCompat="@android:drawable/btn_star_big_on"
                    tools:ignore="DuplicateIds" />

            </LinearLayout>
            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FD050505"
                app:srcCompat="@mipmap/ss" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="254dp"
                    android:layout_height="73dp"
                    android:text="Sanga appointed as the MCC president | ICC 2019"
                    android:textColor="#FAFFFFFF"
                    android:textSize="18sp" />

                <ImageButton
                    android:id="@+id/imageButton20"
                    android:layout_width="50dp"
                    android:layout_height="40dp"
                    android:background="#00FFFFFF"
                    app:srcCompat="@drawable/ic_thumbs_up_down_black_24dp"
                    tools:ignore="VectorDrawableCompat" />

                <ImageButton
                    android:id="@+id/imageButton11"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="1"
                    android:background="#00FFFFFF"
                    app:srcCompat="@android:drawable/btn_star_big_on"
                    tools:ignore="DuplicateIds" />

            </LinearLayout>


        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/sort"
        android:layout_width="124dp"
        android:layout_height="35dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="250dp"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="250dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="154dp"
        android:background="@drawable/ash_back_ash_boader"
        android:text="@string/action_sort" />

    <Button
        android:id="@+id/button3"
        android:layout_width="143dp"
        android:layout_height="34dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="95dp"
        android:layout_marginLeft="173dp"
        android:layout_marginRight="95dp"
        android:layout_marginStart="173dp"
        android:layout_marginTop="154dp"
        android:background="@drawable/ash_back_ash_boader"
        android:text="@string/action_filter" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="263dp"
        android:layout_height="66dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="119dp"
        android:layout_marginRight="119dp"
        android:layout_marginTop="79dp"
        android:hint="search here......"
        android:inputType="textPersonName"
        android:selectAllOnFocus="false"
        android:singleLine="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:visibility="visible" />

</RelativeLayout>